# Default OpenNSA to contact
service=http://localhost:9080/NSI/services/ConnectionService
# Name your client uses
provider=OpenNSA-JHClient
# Name of the provider you are contacting
requester=exampledomain
# Full path to the topology file
topology=/some/path/example-topology.owl
# Full path to the WSDL files
wsdl=/usr/local/share/nsi/wsdl
# Default Bandwidth to request
bandwidth=200
# Default host to connect from
host=localhost
# Default port number to connect from
port=7080
# Default starttime (in seconds)
starttime=+20
# Default endtime (in seconds)
endtime=+260
